{
  "rules": {
    ".read": false,
    ".write": false,
    "registeredGroup": {
      "$groupId": {
        ".indexOn": ".value"
      },
      ".indexOn": [
        "id",
        "title",
        "type",
        "service/automated_quotes"
      ]
    },
    "poll": {
      ".indexOn": [
        "groupId"
      ]
    },
    "mentionedTicker": {
      ".read": true,
      "$groupId": {
        ".indexOn": [
          "createdOn",
          "day",
          "symbol",
          "userId"
        ]
      }
    },
    "mentionedTickerNormalized": {
      ".read": true
    }
  }
}