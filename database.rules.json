{
  "rules": {
    ".read": false,
    ".write": false,
    "registeredGroup": {
      "$groupId": {
        ".indexOn": ".value"
      }
    },
    "poll": {
      ".indexOn": [
        "groupId"
      ]
    },
    "mentionedTicker": {
      "$groupId": {
        ".indexOn": [
            "day"
        ]
      }
    }
  }
}